package {		[SWF(width="1168", height="700", frameRate="60")]			import away3d.cameras.Camera3D;	import away3d.cameras.lenses.PerspectiveLens;	import away3d.containers.View3D;	import away3d.debug.AwayStats;	import away3d.entities.*;	import away3d.extrusions.SkinExtrude;	import away3d.materials.*;	import away3d.primitives.WireframeGrid;		import flash.display.*;	import flash.events.*;	import flash.geom.Vector3D;
		public class SkinExtrudeTest extends MovieClip	{		[Embed(source="../embeds/terrainGrassBlend.jpg")]		private var GrassBlend : Class;				private var _skinextrude:SkinExtrude;		private var _view : View3D;		private var camera:Camera3D;		private var origin:Vector3D = new Vector3D(0,0,0);		private var wave:Number = 0;				public function SkinExtrudeTest():void		{			addEventListener(Event.ADDED_TO_STAGE, init);		}				private function init(e:Event):void		{			removeEventListener(Event.ADDED_TO_STAGE, init);			initView();			populate();						this.addEventListener(Event.ENTER_FRAME, handleEnterFrame);		}				private function initView():void		{			_view = new View3D();			_view.antiAlias = 2;			_view.backgroundColor = 0x333333;			camera = _view.camera;			camera.lens = new PerspectiveLens();			camera.x = 500;			camera.y = 120;			camera.z = 500;			addChild(_view);			addChild(new AwayStats(_view));						camera.lookAt(new Vector3D(0,0,0));			camera.lens.near = 10;			camera.lens.far = 3000;		}				private function populate() : void		{			var wireFrameGrid:WireframeGrid = new WireframeGrid(50, 500, 1, 0x985555, WireframeGrid.PLANE_XZ, true);			_view.scene.addChild(wireFrameGrid);						var mat:BitmapMaterial = new BitmapMaterial(new GrassBlend().bitmapData);						var start:Vector.<Vector3D> = new Vector.<Vector3D>();			var middle:Vector.<Vector3D> = new Vector.<Vector3D>();			var end:Vector.<Vector3D> = new Vector.<Vector3D>();			//multidimentional vector			var vSteps:Vector.<Vector.<Vector3D>> = Vector.<Vector.<Vector3D>>([start, middle, end]);						var index:uint = 0;			for(var i:uint = 0;i<10;++i){				start[i] = new Vector3D(-300, 5, -250 +(50*i));				middle[i] = new Vector3D(0, 5*wave, -250 +(50*i));				end[i] = new Vector3D(300, -5, -250 +(50*i));				wave +=6;				index++;			} 						/* another example feed with 3 steps, 2 entries			start[0] = new Vector3D(-300, 20, -250);			start[1] = new Vector3D(-300, 20, 250);						middle[0] = new Vector3D(0, 50, -350);			middle[1] = new Vector3D(0, 50, 350);						end[0] = new Vector3D(300, 20, -250);			end[1] = new Vector3D(300, 20, 250);			*/						//material:BaseMaterial, vectors:Vector.<Vector.<Vector3D>>, recenter:Boolean = false, closepath:Boolean = false, coverall:Boolean = false, flip:Boolean = false			_skinextrude = new SkinExtrude(mat, vSteps, 2, true, false, true, false);			_skinextrude.x = _skinextrude.z = 0;			_skinextrude.y = 0;			_view.scene.addChild(_skinextrude);		}				private function handleEnterFrame(e : Event) : void		{			_view.camera.position = origin;			_view.camera.rotationY += .5;			_view.camera.moveBackward(1000);						_view.render();		}			}}